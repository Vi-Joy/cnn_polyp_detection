net:"train.prototxt"
test_iter: 189
# make test net, but don't invoke it from the solver itself
test_interval: 9999999999
display: 200
#average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax
base_lr: 1e-10
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 150000
weight_decay: 0.0005
snapshot: 20000
snapshot_prefix: "snapshot/train"
test_initialization: false
